{
    "parameters": {
        "num_processes": {
            "min": 4,
            "max": 12,
            "step": 2,
            "description": "Number of parallel processes to run"
        },
        "records_per_second": {
            "min": 10000,
            "max": 50000,
            "step": 10000,
            "description": "Records per second to generate"
        },
        "total_records": {
            "min": 500000,
            "max": 5000000,
            "step": 500000,
            "description": "Total number of records to generate"
        },
        "duplication_rate": {
            "min": 0.1,
            "max": 0.1,
            "step": 0.1,
            "description": "Rate of duplicate records (0.1 = 10% duplicates)"
        },
        "deduplication_window": {
            "values": ["1h", "4h"],
            "description": "Time window for deduplication"
        },
        "max_batch_size": {
            "values": [5000],
            "description": "Max batch size for the sink"
        },
        "max_delay_time": {
            "values": ["10s"],
            "description": "Max delay time for the sink"
        }
    },
    "test_combinations": {
        "strategy": "grid",
        "max_combinations": 10,
        "required_parameters": ["num_processes", "records_per_second", "total_records"]
    }
} 